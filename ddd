guardarOrderNumber(orderNumber: string): void {
  const orderNumbersKey = 'orderNumbers';
  let orderNumbers: string[] = [];

  const orderNumbersString = sessionStorage.getItem(orderNumbersKey);
  if (orderNumbersString) {
    orderNumbers = JSON.parse(orderNumbersString) as string[];
  }

  orderNumbers.push(orderNumber);

  sessionStorage.setItem(orderNumbersKey, JSON.stringify(orderNumbers));
}


obtenerOrderNumbers(): string[] {
  const orderNumbersKey = 'orderNumbers';
  const orderNumbersString = sessionStorage.getItem(orderNumbersKey);

  if (orderNumbersString) {
    // Si existe un valor en el sessionStorage, lo convertimos a un arreglo de strings
    const orderNumbers = JSON.parse(orderNumbersString) as string[];
    return orderNumbers;
  } else {
    // Si no existe ningún valor en el sessionStorage, devolvemos un arreglo vacío
    return [];
  }
}
ProcesarPago(): void {
  // Obtener el orderNumber desde donde corresponda
  const orderNumber = 'EjemploOrderNumber';

  // Guardar el orderNumber en el sessionStorage
  this.sessionStorageService.guardarOrderNumber(orderNumber);

  // Resto del código de procesamiento del pago
  // ...
}

